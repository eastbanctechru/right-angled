{
    "name": "right-angled",
    "version": "1.0.0",
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    },
    "scripts": {
        "ng": "ng",
        "conventional-changelog": "conventional-changelog",
        "prettier": "prettier --write \"./projects/**/!(*.d.ts|*.png|*.svg|*.ico|*.html|.gitkeep|*.tsfake)\"",
        "docs": "ng build right-angled && ng build right-angled-demo --prod --baseHref=/right-angled/",
        "changelog": "npm run conventional-changelog -- -p angular -i CHANGELOG.md -s",
        "copy-assets": "copy .\\LICENSE .\\dist\\right-angled && copy .\\README.md .\\dist\\right-angled && copy .\\CHANGELOG.md .\\dist\\right-angled",
        "pack": "ng build right-angled && npm run copy-assets && cd dist/right-angled && npm pack",
        "coveralls": "cat ./coverage/right-angled/lcov.info | ./node_modules/.bin/coveralls",
        "ci": "ng lint && ng build && ng build right-angled-demo --prod --baseHref=/right-angled/ && ng test right-angled --watch=false --codeCoverage=true",
        "preversion": "npm run prepush",
        "version": "git add -A dist && npm run changelog && git add CHANGELOG.md",
        "postversion": "git push && git push --tags && conventional-github-releaser -p angular"
    },
    "dependencies": {
        "@angular/animations": "~7.2.0",
        "@angular/common": "~7.2.0",
        "@angular/compiler": "~7.2.0",
        "@angular/core": "~7.2.0",
        "@angular/forms": "~7.2.0",
        "@angular/platform-browser": "~7.2.0",
        "@angular/platform-browser-dynamic": "~7.2.0",
        "@angular/router": "~7.2.0",
        "alertify": "^0.3.0",
        "angulartics2": "^7.4.1",
        "bootstrap": "^3.3.7",
        "classlist.js": "^1.1.20150312",
        "clipboard": "^2.0.4",
        "core-js": "^2.5.4",
        "e2e4": "^2.5.0",
        "normalize.css": "^8.0.1",
        "prismjs": "^1.15.0",
        "rxjs": "~6.4.0",
        "tslib": "^1.9.0",
        "web-animations-js": "^2.3.1",
        "zone.js": "~0.8.26"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.13.0",
        "@angular-devkit/build-ng-packagr": "~0.13.0",
        "@angular/cli": "~7.3.1",
        "@angular/compiler-cli": "~7.2.0",
        "@angular/language-service": "~7.2.0",
        "@types/alertify": "^0.3.30",
        "@types/jasmine": "~2.8.8",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "~8.9.4",
        "@types/sinon": "^7.0.6",
        "codelyzer": "~4.5.0",
        "conventional-changelog-cli": "^2.0.12",
        "conventional-github-releaser": "^3.1.2",
        "coveralls": "^3.0.2",
        "cz-conventional-changelog": "^2.1.0",
        "jasmine-core": "~2.99.1",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~3.1.1",
        "karma-chrome-launcher": "~2.2.0",
        "karma-coverage-istanbul-reporter": "~2.0.1",
        "karma-jasmine": "~1.1.2",
        "karma-jasmine-html-reporter": "^0.2.2",
        "ng-packagr": "^4.2.0",
        "prettier": "^1.16.4",
        "protractor": "~5.4.0",
        "sinon": "^7.2.3",
        "ts-node": "~7.0.0",
        "tsickle": ">=0.34.0",
        "tslib": "^1.9.0",
        "tslint": "~5.11.0",
        "typescript": "~3.2.2"
    }
}
